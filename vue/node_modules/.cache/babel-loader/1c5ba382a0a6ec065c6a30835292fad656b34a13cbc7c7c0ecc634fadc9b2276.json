            </div>\n\n            <div style=\"color: #666666ff; margin-top: 20px\">\n              商家：<a\n                href=\"#\"\n                @click=\"navTo('/front/business?id=' + goodsData.businessId)\"\n                >{{ goodsData.businessName }}</a\n              >\n            </div>\n            <div style=\"color: #666666ff; margin-top: 20px\">\n              分类：<a\n                href=\"#\"\n                @click=\"navTo('/front/type?id=' + goodsData.typeId)\"\n                >{{ goodsData.typeName }}</a\n              >\n            </div>\n            <div style=\"color: #666666ff; margin-top: 20px\">\n              <el-button type=\"warning\" @click=\"addCart\">加入购物车</el-button>\n              <el-button type=\"warning\" @click=\"collect\">收藏</el-button>\n            </div>\n          </el-col>\n        </el-row>\n      </div>\n      <div style=\"padding: 15px 20px\">\n        <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\n          <el-tab-pane label=\"宝贝详情\" name=\"first\">\n            <div\n              style=\"padding: 10px 175px\"\n              v-html=\"goodsData.description\"\n            ></div>\n          </el-tab-pane>\n          <el-tab-pane label=\"宝贝评价\" name=\"second\">\n            <div style=\"margin-top: 10px\">\n              <div\n                style=\"margin-top: 20px\"\n                v-for=\"(item, index) in commentData\"\n                :key=\"index\"\n              >\n                <div style=\"display: flex\">\n                  <div style=\"width: 40px\">\n                    <img\n                      :src=\"item.userAvatar\"\n                      alt=\"\"\n                      style=\"height: 40px; width: 40px; border-radius: 50%\"\n                    />\n                  </div>\n                  <div style=\"width: 200px; margin-left: 10px\">\n                    <div\n                      style=\"\n                        font-weight: 700;\n                        font-size: 17px;\n                        color: #000000ff;\n                      \"\n                    >\n                      {{ item.userName }}\n                    </div>\n                    <div style=\"color: #7a7a7aff\">{{ item.time }}</div>\n                  </div>\n                </div>\n                <div style=\"margin-top: 15px; font-size: 16px\">\n                  {{ item.content }}\n                </div>\n              </div>\n            </div>\n          </el-tab-pane>\n        </el-tabs>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NavBar from \"@/component/NavBar.vue\";\n\nexport default {\n  components: {\n    NavBar,\n  },\n  data() {\n    let goodsId = this.$route.query.id;\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      goodsId: goodsId,\n      goodsData: {},\n      activeName: \"first\",\n      commentData: [],\n    };\n  },\n  mounted() {\n    this.loadGoods();\n    this.loadComments();\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    loadGoods() {\n      this.$request.get(\"/goods/selectById?id=\" + this.goodsId).then((res) => {\n        if (res.code === \"200\") {\n          this.goodsData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleClick(tab, event) {\n      this.activeName = tab.name;\n    },\n    collect() {\n      let data = {\n        userId: this.user.id,\n        businessId: this.goodsData.businessId,\n        goodsId: this.goodsId,\n      };\n      this.$request.post(\"/collect/add\", data).then((res) => {\n        if (res.code === \"200\") {\n          this.$message.success(\"收藏成功\");\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    addCart() {\n      let data = {\n        num: 1,\n        userId: this.user.id,\n        goodsId: this.goodsId,\n        businessId: this.goodsData.businessId,\n      };\n      this.$request.post(\"/cart/add\", data).then((res) => {\n        if (res.code === \"200\") {\n          this.$message.success(\"操作成功\");\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    navTo(url) {\n      location.href = url;\n    },\n    loadComments() {\n      this.$request\n        .get(\"/comment/selectByGoodsId?id=\" + this.goodsId)\n        .then((res) => {\n          if (res.code === \"200\") {\n            this.commentData = res.data;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n    },\n  },\n};\n</script>"],"mappings":"AA0HA,OAAAA,MAAA;AAEA;EACAC,UAAA;IACAD;EACA;EACAE,KAAA;IACA,IAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAR,OAAA,EAAAA,OAAA;MACAS,SAAA;MACAC,UAAA;MACAC,WAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;IACA,KAAAC,YAAA;EACA;EACA;EACAC,OAAA;IACAF,UAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,gCAAAjB,OAAA,EAAAkB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAX,SAAA,GAAAU,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,YAAAC,GAAA,EAAAC,KAAA;MACA,KAAAhB,UAAA,GAAAe,GAAA,CAAAE,IAAA;IACA;IACAC,QAAA;MACA,IAAA7B,IAAA;QACA8B,MAAA,OAAAzB,IAAA,CAAAD,EAAA;QACA2B,UAAA,OAAArB,SAAA,CAAAqB,UAAA;QACA9B,OAAA,OAAAA;MACA;MACA,KAAAgB,QAAA,CAAAe,IAAA,iBAAAhC,IAAA,EAAAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAW,OAAA;QACA;UACA,KAAAX,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAU,QAAA;MACA,IAAAlC,IAAA;QACAmC,GAAA;QACAL,MAAA,OAAAzB,IAAA,CAAAD,EAAA;QACAH,OAAA,OAAAA,OAAA;QACA8B,UAAA,OAAArB,SAAA,CAAAqB;MACA;MACA,KAAAd,QAAA,CAAAe,IAAA,cAAAhC,IAAA,EAAAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAW,OAAA;QACA;UACA,KAAAX,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAY,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAtB,aAAA;MACA,KAAAE,QAAA,CACAC,GAAA,uCAAAjB,OAAA,EACAkB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,WAAA,GAAAQ,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CA